{
  "name": "anura-server",
  "version": "0.4.3",
  "keywords": [
    "anura",
    "configuration",
    "config",
    "config server"
  ],
  "description": "Anura is a configuration server that make dealing with configs file a lot easier ",
  "repository": "AnuraConfig/anura-server",
  "proxy": "http://localhost:4000",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "@material-ui/icons": "^3.0.1",
    "apollo-boost": "^0.1.22",
    "apollo-server": "^2.2.5",
    "apollo-server-express": "^2.3.1",
    "classnames": "^2.2.6",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.3",
    "js-yaml": "^3.12.1",
    "mongoose": "^5.4.2",
    "path": "^0.12.7",
    "react": "^16.6.3",
    "react-ace": "^6.3.2",
    "react-apollo": "^2.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-toastify": "^4.5.1",
    "socket.io": "^2.2.0",
    "socket.io-redis": "^5.2.0",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yaml": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.3.0",
    "react-scripts": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-client": "react-scripts build",
    "test": "mocha --require babel-register --require babel-polyfill --recursive ./test",
    "test-watch": "mocha --require babel-register --require babel-polyfill --recursive ./test --watch",
    "coverage": "nyc  --reporter=html --reporter=text npm test",
    "eject": "react-scripts eject",
    "start-server": "set NODE_ENV=development & nodemon ./server/server.js --exec babel-node",
    "debug-server": "set NODE_ENV=development & nodemon --inspect ./server/server.js --exec babel-node",
    "build-server": "rm -rf ./lib/* &babel ./server -d lib",
    "build": "npm run build-client && npm run build-server",
    "prod": "node ./lib/server.js",
    "postinstall": "node ./bin/scripts/post-install.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "bin": {
    "anura": "./bin/anura-server.js"
  },
  "preferGlobal": true,
  "author": "HenShalom",
  "license": "MIT",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
